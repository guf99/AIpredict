{
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "volixai.png",
      "use": "@vercel/static"
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/predictions", "dest": "/api/predictions.js" },
    { "src": "/api/polymarket", "dest": "/api/polymarket.js" },
    { "src": "/api/kalshi", "dest": "/api/kalshi.js" },
    { "src": "/api/health", "dest": "/api/health.js" },
    { "src": "/volixai.png", "dest": "/volixai.png" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
